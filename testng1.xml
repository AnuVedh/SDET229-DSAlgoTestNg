<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DSALGO Suite" thread-count="5" parallel="tests">

<listeners>
    
    <listener class-name="Utils.ExtentReport"/>
     <listener class-name="listeners.Chaintestlistener2"/>
   

   
</listeners>

<test name="DSAlgoTest-chrome" >
  <parameter name="browser" value="chrome"></parameter>
    <classes>
     
     <class name="TestCase.TestCase_Arrays"/>
      <class name="TestCase.TestCase_Home"/>
       <class name="TestCase.TestCase_Registrationpage"/>
        <class name="TestCase.TestCase_SignIn"/>
         <class name="TestCase.TestCase_Queue"/>
      <class name="TestCase.TestCase_Graph"/>  
     <class name="TestCase.TestCase_Queue"/>
    </classes>
  </test> <!-- Test -->
   
  <test name="DSAlgoTest-edge" >
  <parameter name="browser" value="edge"></parameter>
    <classes>
      <class name="TestCase.TestCase_Arrays"/>
      <class name="TestCase.TestCase_Home"/>
      <class name="TestCase.TestCase_SignIn"/>
       <class name="TestCase.TestCase_Registrationpage"/>
       <class name="TestCase.TestCase_Queue"/>
      <class name="TestCase.TestCase_Graph"/>
      
    </classes>  
  </test>  <!-- Test -->
 
   <test name="DSAlgoTest-firefox" >
  <parameter name="browser" value="firefox"></parameter>
    <classes>
     <class name="TestCase.TestCase_SignIn"/>
       <class name="TestCase.TestCase_Registrationpage"/>
     <class name="TestCase.TestCase_Queue"/>
      <class name="TestCase.TestCase_Graph"/>
    </classes>  
  </test>   <!-- Test -->
</suite> <!-- Suite -->

